{
  "name": "deck.zone",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 3456",
    "build": "ng build",
    "test": "npm run build:lang && npm run lint && npm run build:app",
    "lint": "ng lint",
    "build:app": "npm run build",
    "build:lang": "npm run build:lang:directives && npm run build:lang:map && nearleyc decklang/decklang.ne -o src/decklang/decklang.js && npm run build:lang:help",
    "build:lang:directives": "node gen/build-lang-directives > decklang/_directives.ne",
    "build:lang:help": "node gen/build-lang-help > src/app/help/help.json",
    "build:lang:map": "node gen/build-lang-map > src/decklang/plugins.json",
    "deploy": "npm run build:app && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "@toverux/ngx-sweetalert2": "^4.0.0",
    "angular-font-awesome": "^3.1.2",
    "bootstrap": "^4.2.1",
    "brace": "^0.11.1",
    "core-js": "^2.5.4",
    "filesaverjs": "^1.2.0",
    "firebase": "^5.7.2",
    "font-awesome": "^4.7.0",
    "human-readable-ids": "^1.0.4",
    "jszip": "^3.1.5",
    "lodash": "^4.17.11",
    "mathjs": "^5.4.0",
    "ngx-ace-wrapper": "^7.2.0",
    "ngx-bootstrap": "^3.1.4",
    "ngx-webstorage": "^3.0.0-beta.14",
    "npm": "^6.5.0",
    "rxjs": "~6.3.3",
    "sweetalert2": "^7.33.1",
    "tabletop": "github:seiyria/tabletop",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.2.0",
    "@angular-builders/dev-server": "^7.2.1",
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "firebase-tools": "^6.2.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nearley": "^2.16.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "worker-loader": "^2.0.0"
  }
}
